etree = no

# On Caltech Resnick HPC, load the following module before compilation:
# module load openmpi/4.1.5-gcc-11.3.1-xjqcckv 

# CXX = C++ compiler (Open MPI)
CXX = mpicxx

# FC  = Fortran compiler (Open MPI)
FC = mpifort

# SW4ROOT = path to third party libraries (used when proj=yes)
proj = yes
SW4ROOT = /central/software9/spack/opt/spack/linux-rhel9-x86_64/gcc-11.3.1/proj-8.1.0-53id2wb744oxnqiwesxyg77wtsp7ena3

# HDF5ROOT = path to hdf5 library and include files (used when hdf5=yes)
hdf5 = yes
HDF5ROOT = /central/software9/spack/opt/spack/linux-rhel9-x86_64/gcc-11.3.1/hdf5-1.14.3-juor6c7dn2dhb6jwk6jgagom3scde677

# FFTWHOME = path to fftw library and include files (used when fftw=yes)
fftw = yes
FFTWHOME = /central/software9/spack/opt/spack/linux-rhel9-x86_64/gcc-11.3.1/fftw-3.3.10-ue6oddl67exxsaz27qdqk2qcdumdhpmk

JPEG_PATH = /central/software9/spack/opt/spack/linux-rhel9-x86_64/gcc-11.3.1/libjpeg-turbo-3.0.0-fykwcrceqhi6w3itiadp4zmwc4udujo6
EXTRA_LINK_FLAGS = -L${JPEG_PATH}/lib64 -ljpeg -Wl,-rpath=${FFTWHOME}/lib64 -Wl,-rpath=${SW4ROOT}/lib64 -L$/usr/lib64 -llapack -lblas -lgfortran

# EXTRA_CXX_FLAGS  = additional c++ compiler flags
# EXTRA_FORT_FLAGS = additional fortran compiler flags

optdir := optimize_resnick
